{
  "routes": [
    {
      "src": "/(.*)\\.css$",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)\\.js$",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)\\.jsx$",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ]
}
